from Verdbolga import *

def calculate(innborgun, timi, vextir, verdtrygg):
    avg_inf = avgInflation("1993","2014")
    
    years = timi/12
    rVextir = vextir/100
    rInf = avg_inf/100
    
    if(verdtrygg) :
        Evt = ((1+vextir)**timi)*((1+avg_inf)**years)*innborgun
        return Evt
        
    else :
        E = ((1+vextir)**years)*innborgun
        return E
    
print(calculate(1000,14,3.00,False))
    
    
print((1))
